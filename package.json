{
  "name": "nodecg-destinyStats",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/nomad980/nodecg-destinyStats.git"
  },
  "homepage": "http://thebrooder.org/",
  "contributors": [
    "El_Eskimo",
  ],
  "description": "Show's Destiny Character's information.",
  "license": "MIT",
  "nodecg": {
    "compatibleRange": "~0.7.0",
    "dashboardPanels": [
      {
        "name": "schedule",
        "title": "Schedule",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "schedule.html"
      },
      {
        "name": "edit-current-run",
        "title": "Edit Current Run",
        "width": 4,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-current-run.html"
      },
      {
        "name": "advertisements",
        "title": "Advertisements",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "advertisements.html"
      },
      {
        "name": "checklist",
        "title": "Checklist",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "checklist.html"
      },
      {
        "name": "omnibar",
        "title": "Omnibar",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "omnibar.html"
      },
      {
        "name": "total",
        "title": "Total",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "total.html"
      },
      {
        "name": "edit-total",
        "title": "Edit Total",
        "width": 2,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-total.html"
      },
      {
        "name": "twitter",
        "title": "Twitter",
        "width": 3,
        "headerColor": "#55acee",
        "file": "twitter.html"
      },
      {
        "name": "interview",
        "title": "Interview",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "interview.html"
      },
      {
        "name": "nowplaying",
        "title": "Now Playing",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "nowplaying.html"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      },
      {
        "file": "custom_controls/stopwatches/index.html",
        "width": 1280,
        "height": 720
      }
    ]
  },
}
